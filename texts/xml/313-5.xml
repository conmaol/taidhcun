      <w id="_313-5_d2e329099">aig</w>
      <w id="_313-5_d2e329103">Dòmhnall</w>
      <w id="_313-5_d2e329112" ref="a-nis">a-nis</w>
      <pc>,</pc>
      <pc>”</pc>
      <w id="_313-5_d2e329122" ref="arsa">ars</w>
      <w id="_313-5_d2e329125" ref="a-poss">a</w>
      <w id="_313-5_d2e329128" ref="bean">bhean</w>
      <pc>,</pc>
